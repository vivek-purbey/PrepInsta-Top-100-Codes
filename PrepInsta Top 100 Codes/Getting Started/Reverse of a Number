#include<bits/stdc++.h>
using namespace std;
int main() {
    int tc; cin>>tc; while(tc--) {
        int num; cin>>num;
        int sum=0;
        while(num!=0) {
            int rem=num%10; //last digit
            cout<<rem; //print each last digit one by one
            num/=10; //last digit remove
        }
        cout<<endl;
    }
    return 0;
}

//Method-2 by using string : 
#include<bits/stdc++.h>
using namespace std;
int main() {
    int tc; cin>>tc; while(tc--) {
        string s;
        cin>>s;
        for(int i=(s.size()-1);i>=0;i--) { //reverse given string
            cout<<s[i];
        }
        cout<<endl;
    }
    return 0;
}
