// Method-1:
#include<bits/stdc++.h>
using namespace std;
int main() {
    int tc; cin>>tc; while(tc--) {
        int year; cin>>year;
        if(year%400==0 or (year%4==0 and year%100!=0)) cout<<"Leap Year"<<endl;
        else cout<<"Not a Leap Year"<<endl;
    }
    return 0;
}
