#include<iostream>
#include<algorithm>
using namespace std;
int main() {
	int tc; cin>>tc; while(tc--) {
		int num; cin>>num;
		int arr[num];
		for(int i=0;i<num;i++) {
			cin>>arr[i];
		}
		// print array elements in reverse order
		for(int i=num-1;i>=0;i--) {
			cout<<arr[i]<<" ";
		}
		cout<<endl;
	}
	return 0;
}

// using inbuilt()
#include<iostream>
#include<algorithm>
using namespace std;
int main() {
	int tc; cin>>tc; while(tc--) {
		int num; cin>>num;
		int arr[num];
		for(int i=0;i<num;i++) {
			cin>>arr[i];
		}
		// print array elements in reverse order
		reverse(arr, arr+num);
		for(int i=0;i<num;i++) {
			cout<<arr[i]<<" ";
		}
		cout<<endl;
	}
	return 0;
}
// in both case time complexity is O(n) and space complexity is O(1).
